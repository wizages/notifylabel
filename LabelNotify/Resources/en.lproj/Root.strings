<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Section Title. -->
	<key>TITLE</key>
	<string>LabelNotify Settings</string>

	<!-- Switch to enable or disable the tweak -->
	<key>ENABLE_LABEL</key>
	<string>Enabled</string>

	<!-- Label for the color settings page -->
	<key>COLORS</key>
	<string>Color Settings</string>

	<!-- Label for the app settings page. -->
	<key>APP_SETTINGS</key>
	<string>Per App Settings</string>

	<!-- Label for the Miscellaneous Settings page. -->
	<key>MISC_TITLE</key>
	<string>Miscellaneous Settings</string>

	<!-- Respring button label -->
	<key>RESPRING_BUTTON</key>
	<string>Respring to apply changes</string>

	<!-- Label for the Makers Settings page. -->
	<key>MAKERS</key>
	<string>Makers</string>

	<!-- Label for the Support Settings page. -->
	<key>SUPPORT</key>
	<string>Support</string>

	<!-- Tweet that is sent when you press the heart. -->
	<key>TWEET_STRING</key>
	<string>Why have badges when you can have fancy labels #LabelNotify by @wizages</string>

	<!-- Question to ask if a person wants to respring -->
	<key>RESPRING_QUESTION</key>
	<string>Are you sure you want to respring?</string>

	<!-- Yes (confirm button) -->
	<key>YES</key>
	<string>Yes</string>

	<!-- No (cancel button) -->
	<key>NO</key>
	<string>no</string>
</dict>
</plist>