<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//DE" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Section Title. -->
	<key>TITLE</key>
	<string>LabelNotify Einstellungen</string>

	<!-- Switch to enable or disable the tweak -->
	<key>ENABLE_LABEL</key>
	<string>Aktiviert</string>

	<!-- Label for the color settings page -->
	<key>COLORS</key>
	<string>Farbeinstellungen</string>

	<!-- Label for the app settings page. -->
	<key>APP_SETTINGS</key>
	<string>Pro App Einstellungen</string>

	<!-- Label for the Miscellaneous Settings page. -->
	<key>MISC_TITLE</key>
	<string>Sonstige Einstellungen</string>

	<!-- Respring button label -->
	<key>RESPRING_BUTTON</key>
	<string>Benutzeroberflache neustarten um Anderungen zu aktivieren</string>

	<!-- Label for the Makers Settings page. -->
	<key>MAKERS</key>
	<string>Entwickler</string>

	<!-- Label for the Support Settings page. -->
	<key>SUPPORT</key>
	<string>Support</string>

	<!-- Tweet that is sent when you press the heart. -->
	<key>TWEET_STRING</key>
	<string>Warum Kennzeichensymbole, wenn man schicke Beschriftungen haben kann #LabelNotify von @wizages</string>

	<!-- Question to ask if a person wants to respring -->
	<key>RESPRING_QUESTION</key>
	<string>Sind Sie sicher, dass sie die Benutzeroberflache neustarten wollen?</string>

	<!-- Yes (confirm button) -->
	<key>YES</key>
	<string>Ja</string>

	<!-- No (cancel button) -->
	<key>NO</key>
	<string>Nein</string>
</dict>
</plist>
