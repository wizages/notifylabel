<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Main Title and Section Title (miscellaneous is abbreviated because it is too long.)-->
	<key>MISC_TITLE</key>
	<string>إعدادات أخرى</string>

	<!-- Default notifications badge name -->
	<key>DEFAULT_VALUE</key>
	<string>الإشعارات</string>

	<!-- Default notfications label -->
	<key>DEFAULT_LABEL</key>
	<string>الإسم الإفتراضي</string>

	<!-- Label for a switch that disables badges -->
	<key>BADGE_SWITCH_LABEL</key>
	<string>تفعيل الإشعارات</string>

	<!-- Label for a switch that disables app names -->
	<key>APPNAME_SWITCH_LABEL</key>
	<string>إخفاء أسماء التطبيقات</string>

	<!-- Label for a switch that disables pluralization of strings. This means we make it more than one. -->
	<key>PLURAL_SWITCH_LABEL</key>
	<string>جميع</string>

	<!-- Plural string means what is the generic pluralizer for your language. If this is not available put in N/A-->
	<key>PLURAL_GROUP_LABEL</key>
	<string>الجميع</string>

	<!-- Generic way to make a string plural. If this is not available put in N/A-->
	<key>PLURAL</key>
	<string>الكل</string>
</dict>
</plist>